        <tri_webbot><![CDATA[$aUser = userInfo(@@USER_LOGGED);
//$user_name = $aUser['firstname'] .' '. $aUser['lastname'];
$requester_email = $aUser['mail'];
//print_r($requester_email); die();
$sqlQueryUser = executeQuery("SELECT * FROM PMT_DV_DAY_CARE_QUEUE WHERE REQ_EMAIL = '$requester_email'");
//echo'<pre>';
//print_r($sqlQueryUser); die();
if (is_array($sqlQueryUser) and count($sqlQueryUser) > 0) {
	@@Grid_ChildName		= $sqlQueryUser[1]['CHILD_NAME'];
	@@Grid_ChildGender   	= $sqlQueryUser[1]['CHILD_GENDER'];
