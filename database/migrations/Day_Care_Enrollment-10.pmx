      <record>
        <tri_uid>2687508555d2d9709e59a85004008272</tri_uid>
        <pro_uid>6636409255d2b030f319111003840103</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$APPUID		= @@APPLICATION;
$EMP_NAME	= @@Req_Name;
$EMP_ID		= @@Req_ID;
$EMP_EMAIL	= @@Req_Email;
$EMP_GENDER	= 'Male';
$REQ_DATE	= @@Request_Date;
$INFO_GRID	= @@children_Info;

foreach($INFO_GRID as $ROW){
	$CHILD_ID			= $ROW['childName'];
	$CN					= executeQuery("SELECT CHILD_NAME FROM PMT_DV_DAY_CARE_EMPLOYEE_CHILD_INFO WHERE ID = '$CHILD_ID'");
	$CHILD_NAME			= $CN[1]['CHILD_NAME'];
	//echo $CHILD_NAME; die();	
	$CHILD_GENDER		= $ROW['childGender'];
	$DATE_OF_BIRTH		= $ROW['childBirthDay'];
	$AGE				= $ROW['childAge'];
	//echo $AGE; die();
	$DATE_OF_ENROLLMENT	= $ROW['childEnrollmentDate'];
	
	$REQ_ATTACHMENT		= '';
	$bserver 			= (G::is_https() ? 'https://':'http://').$_SERVER['SERVER_NAME'].':'.$_SERVER['SERVER_PORT'];
$burls = $bserver . PATH_SEP . 'sys' . @@SYS_SYS . PATH_SEP . @@SYS_LANG . PATH_SEP . @@SYS_SKIN . PATH_SEP . 'cases' . PATH_SEP. 'cases_ShowDocument?a=';
	
	$attachment	= $ROW['childInfoAttachment'];
	if(count($attachment)>0){  foreach($attachment as $row ){ $bfilName .= $burls.$row['appDocUid']."||"; } }
	$REQ_ATTACHMENT		= $bfilName;
	
	$saveData	= "INSERT INTO PMT_DV_DAY_CARE_DATA	(APPUID,REQ_NAME,REQ_ID,REQ_EMAIL,REQ_GENDER,CHILD_NAME,CHILD_GENDER,DATE_OF_BIRTH,AGE,DATE_OF_ENROLLMENT,ATTACHMENT,REQUEST_DATE) VALUES('$APPUID','$EMP_NAME','$EMP_ID','$EMP_EMAIL','$EMP_GENDER','$CHILD_NAME','$CHILD_GENDER','$DATE_OF_BIRTH','$AGE','$DATE_OF_ENROLLMENT','$REQ_ATTACHMENT','$REQ_DATE')";
	
	executeQuery($saveData);
	
	$saveQueue	= "INSERT INTO PMT_DV_DAY_CARE_QUEUE (APPUID,REQ_NAME,REQ_ID,REQ_EMAIL,REQ_GENDER,CHILD_NAME,CHILD_GENDER,DATE_OF_BIRTH,AGE,DATE_OF_ENROLLMENT,ATTACHMENT,REQUEST_DATE,STATUS) VALUES ('$APPUID','$EMP_NAME','$EMP_ID','$EMP_EMAIL','$EMP_GENDER','$CHILD_NAME','$CHILD_GENDER','$DATE_OF_BIRTH','$AGE','$DATE_OF_ENROLLMENT','$REQ_ATTACHMENT','$REQ_DATE',1)";
	executeQuery($saveQueue);	
}]]></tri_webbot>
