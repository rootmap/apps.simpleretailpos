	if(count($CheckReqEmail)>0)
	{
		$email_array=array();
		foreach($CheckReqEmail as $row){
			$sqlUserUID=executeQuery("SELECT USR_UID FROM USERS WHERE USR_EMAIL='".$row['REQ_EMAIL']."'");
			
			if(!in_array($sqlUserUID[1]['USR_UID'],$email_array))
			{
				array_push($email_array,$sqlUserUID[1]['USR_UID']);
			}
			
			executeQuery("UPDATE PMT_DV_DAY_CARE_QUEUE SET QUEUE_EXISTS_STATUS='1' WHERE ID='".$row['ID']."'");
			
		}
		
		//print_r($email_array); die();
		
