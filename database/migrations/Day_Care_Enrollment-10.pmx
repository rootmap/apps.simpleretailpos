        <pro_uid>6636409255d2b030f319111003840103</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[@@hiddenAction ='';


$Enrolled	= executeQuery("SELECT * FROM PMT_DV_DAY_CARE_ENROLLED WHERE STATUS = 1 limit 20");
$e=1;
if($Enrolled){
	foreach($Enrolled as $row){
		$enrolled_Grid[$e]	= array(
			'ID'				=> $row['ID'],
			'REQ_EMAIL'			=> $row['REQ_EMAIL'],
			'REQ_NAME'			=> $row['REQ_NAME'],
			'CHILD_NAME'		=> $row['CHILD_NAME'],
			'DATE_OF_BIRTH'		=> $row['DATE_OF_BIRTH'],
			'AGE'				=> $row['AGE'],
			'DATE_OF_ENROLLMENT'=> $row['DATE_OF_ENROLLMENT']
		);
		$e++;
	}
}

@=enrolled_Grid = $enrolled_Grid;


$Queue		= executeQuery("SELECT * FROM PMT_DV_DAY_CARE_QUEUE WHERE STATUS = 1 ORDER BY ID ASC ");
$Q=1;
if($Queue){
	foreach($Queue as $row1){
		
		$feedback="";
		if(!empty($row1['QUEUE_EXISTS_STATUS']))
		{
			if($row1['QUEUE_EXISTS_STATUS']==1)
			{
				$feedback="Request Send for Approval";
			}
			elseif($row1['QUEUE_EXISTS_STATUS']==2)
			{
				$feedback="Approved";
			}
			elseif($row1['QUEUE_EXISTS_STATUS']==3)
			{
				$feedback="Reject";
			}
		}
		
		$meeting_sch_status="Not Eligible";
		
		if(!empty($feedback))
		{
			$meeting_sch_status="Not set yet";
			if(!empty($row1['MEETING_SCHEDULE_STATUS']))
			{
				$meeting_sch_status="Meeting scheduled";
			}
		}
		
		$queue_grid[$Q]	= array(
			'hiddenEmail'		=>$row1['REQ_EMAIL'],
			'QUID'				=>$row1['ID'],
			'QNameOfEmployee'	=>$row1['REQ_NAME'],
			'QNameOfChild'		=>$row1['CHILD_NAME'],
			'QDateOfBirth'		=>$row1['DATE_OF_BIRTH'],
			'QAge'				=>$row1['AGE'],
			'QDateOfEnrollment'	=>$row1['DATE_OF_ENROLLMENT'],
			'QUEUE_EXISTS_STATUS'	=>$row1['QUEUE_EXISTS_STATUS'], 
			'meeting_schedule_status'	=>$meeting_sch_status,
			'EmployeeFeedback'=>$feedback
		);
		$Q++;
	}
}

@=queue_grid	= $queue_grid;


//echo '<pre>';
