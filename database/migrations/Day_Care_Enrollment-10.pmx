        <dyn_content><![CDATA[{"name":"Requester_Request_View","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"1706997085d2b05f5b9b102043103834","name":"Requester_Request_View","description":"","mode":"edit","script":{"type":"js","code":"$('.control-label').css('text-align','left');\n$(\"[id='form[children_Info][1][childEnrollmentDate_label]']\").attr(\"readonly\", false);\n\n$(\"#submit\").find(\"button\").css(\"backgroundColor\", \"#1fbc99\"); \n$(\"#submit\").find(\"button\").css(\"border\", \"1px solid #1ba385\");\n$(\"#submit, #Cancel\").find(\"button\").css(\"color\", \"#fff\");\n$(\"#Cancel\").find(\"button\").css(\"backgroundColor\", \"#e4655f\");\n$(\"#Cancel\").find(\"button\").css(\"border\", \"1px solid #e14333\");\n\n//$(\"[id='form[children_Info][1][TEST_label]']\").setValue('1234');\n\n//$(\"#children_Info\").setValue($(\"#Req_ID\").getValue(),1,1)\n\n$(\"#Req_Email,#Test_ChildName\").hide();\n\nfunction loadDefaultUserGridInfo(initialpaam)\n{\n    var drp='<option value=\"\">Please Wait ...</option>';\n   $(\"[id='form[children_Info][\"+initialpaam+\"][childName]']\").html(drp);\n   var Req_ID=$(\"#Req_ID\").getValue();\n   var sql=\"SELECT * FROM PMT_DV_DAY_CARE_EMPLOYEE_CHILD_INFO WHERE EMPLOYEEID='\"+Req_ID+\"'\";\n   $.ajax({\n      method: \"POST\",\n      dataType:'json',\n      url: \"https://workflow.robi.com.bd/divergent/IoT/getReport.php\",\n      data: {sql:sql}\n    }).done( function( obj ){\n       var drp='<option value=\"\">Please Select</option>';\n     \n       $(\"[id='form[children_Info][\"+initialpaam+\"][childName]']\").html(drp);\n\n       var drp='<option value=\"\">Please Select</option>';\n       $.each(obj,function(index,row){\n\t\t\tdrp+='<option value=\"'+row.ID+'\">'+row.CHILD_NAME+'</option>';\n       });\n     \n     \t$(\"[id='form[children_Info][\"+initialpaam+\"][childName]']\").html(drp);\n    });\n  \n  \n\n}\n\n\n  var curday = function(sp){\n  today = new Date();\n  var dd = today.getDate();\n  var mm = today.getMonth()+1; //As January is 0.\n  var yyyy = today.getFullYear();\n\n  if(dd<10) dd='0'+dd;\n  if(mm<10) mm='0'+mm;\n  return (dd+sp+mm+sp+yyyy);\n  };\n  \n  console.log(curday('/'));\n  console.log(curday('-'));\n\nfunction dateDiff(date) {\n    date = date.split('-');\n    var today = new Date();\n    var year = today.getFullYear();\n    var month = today.getMonth() + 1;\n    var day = today.getDate();\n    var yy = parseInt(date[0]);\n    var mm = parseInt(date[1]);\n    var dd = parseInt(date[2]);\n    var years, months, days;\n    // months\n    months = month - mm;\n    if (day < dd) {\n        months = months - 1;\n    }\n    // years\n    years = year - yy;\n    if (month * 100 + day < mm * 100 + dd) {\n        years = years - 1;\n        months = months + 12;\n    }\n    // days\n    days = Math.floor((today.getTime() - (new Date(yy + years, mm + months - 1, dd)).getTime()) / (24 * 60 * 60 * 1000));\n    //\n    return {years: years, months: months, days: days};\n}\n\n\nfunction loadGridChildrenInfo(children_id,grid_row)\n{\n   var sql=\"SELECT * FROM PMT_DV_DAY_CARE_EMPLOYEE_CHILD_INFO WHERE ID='\"+children_id+\"'\";\n   $.ajax({\n      method: \"POST\",\n      dataType:'json',\n      url: \"https://workflow.robi.com.bd/divergent/IoT/getReport.php\",\n      data: {sql:sql}\n    }).done( function( obj ){\n       console.log(obj[0]);\n        console.log(obj);\n     \t$(\"#children_Info\").setValue(obj[0].CHILD_GENDER,grid_row, 2);\n        $(\"#children_Info\").setValue(obj[0].CHILD_DOB,grid_row,3);\n     \n     \t\n     \n     \tvar ageParam=dateDiff(obj[0].CHILD_DOB);\n     \tconsole.log(ageParam);\n     \tvar age='';\n     \tif(ageParam.years>0)\n        {\n          age+=ageParam.years+' Year';\n        }\n     \n     \tif(ageParam.months>0)\n        {\n          if(age.length>0)\n          {\n            age+=', ';\n          }\n          age+=ageParam.months+' Month';\n        }\n     \t\n     \t$(\"#children_Info\").setValue(age,grid_row,4);\n     \n    });\n}\n\nloadDefaultUserGridInfo(1);\n\n\n$(\"form\").setOnchange(function(field, newVal, oldVal) {\n  var totalRows = $(\"#children_Info\").getNumberRows();\n  for (var i = 1; i <= totalRows; i++) {\n    var childName   = $(\"#children_Info\").getValue(i, 1);\n    console.log(childName);\n    \n    loadGridChildrenInfo(childName,i);\n    \n    \n  }\n});\n\n$(\"#children_Info\").onAddRow(function(newArrayRow, gridObject, indexAdd) {\n    loadDefaultUserGridInfo(indexAdd);\n})\n\n//url: \"https://workflow.robi.com.bd/divergent/dayCare_childInfo\";"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"Day Care Enrollment From","colSpan":6},{"type":"text","variable":"Req_Email","var_uid":"5824440185d303c977ab904000477595","dataType":"string","protectedValue":false,"id":"Req_Email","name":"Req_Email","label":"Email","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"edit","operation":"","datasource":"database","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","dataVariable":"","var_name":"Req_Email","colSpan":6}],[{"type":"form","variable":"","var_uid":"","dataType":"","id":"1676627075d2b0631436b90012599887","name":"Employee_Info","description":"","mode":"edit","script":"","language":"en","externalLibs":"","printable":false,"items":[[{"type":"text","variable":"Req_Name","var_uid":"2498248955d2b0646b87478009684288","dataType":"string","protectedValue":false,"id":"Req_Name","name":"Req_Name","label":"Employee Name","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"view","operation":"","datasource":"database","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","dataVariable":"","var_name":"Req_Name","colSpan":4},{"type":"tex